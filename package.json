{
  "name": "atom-ide-hyperclick",
  "repository": "https://github.com/atom-ide-community/atom-ide-hyperclick",
  "main": "./lib/main.js",
  "version": "1.0.1",
  "description": "Hyperclick UI",
  "author": "Tony Brix <tony@brix.ninja>",
  "atomTestRunner": "./spec/runner",
  "activationHooks": [
    "core:loaded-shell-environment"
  ],
  "consumedServices": {
    "hyperclick.provider": {
      "versions": {
        "0.0.0": "addLegacyProvider"
      }
    },
    "hyperclick": {
      "versions": {
        "0.1.0": "addProvider"
      }
    }
  },
  "providedServices": {
    "hyperclick.observeTextEditor": {
      "versions": {
        "0.0.0": "observeTextEditor"
      }
    }
  },
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint .",
    "lintfix": "eslint . --fix",
    "test": "atom --test spec",
    "bump": "ncu -u"
  },
  "devDependencies": {
    "@types/atom": "^1.40.4",
    "@types/node": "^14.0.23",
    "atom-jasmine3-test-runner": "^4.4.8",
    "eslint": "^7.4.0",
    "eslint-config-atomic": "^1.0.0",
    "eslint-plugin-react": "^7.20.3",
    "npm-check-updates": "^7.0.2",
    "prettier": "^2.0.5",
    "typescript": "^3.9.7"
  },
  "dependencies": {
    "log4js": "^6.3.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "rxjs": "^6.6.0"
  },
  "package-deps": []
}
